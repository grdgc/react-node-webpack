{
  "name": "react-node-webpack",
  "description": "Example app built with React, Node.js and Webpack",
  "author": "Seth Bergman <hello@sethbergman.com>",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "webpack -d --progress --colors --watch",
    "dist": "webpack -p",
    "postinstall": "npm run dist",
    "test": "jest",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js"
  },
  "dependencies": {
    "classnames": "^2.1.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.12.4",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jsx-loader": "^0.13.2",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "normalize.css": "^8.0.0",
    "object-assign": "^4.1.1",
    "qs": "^6.3.0",
    "react": "^16.4.1",
    "react-router": "^4.3.1",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2"
  },
  "devDependencies": {
    "jest-cli": "^23.4.1",
    "nodemon": "^1.3.7",
    "react-tools": "^0.13.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/support/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
